export VERSION_KCCTL=1.0.0.CR3
export VERSION_BYTECODE_VIEWER=2.12
export VERSION_KEYCLOAK=25.0.4
export VERSION_WILDFLY=33.0.1.Final
export VERSION_QUARKUS=3.17.0
export VERSION_SONARQUBE=10.6.0.92116
export VERSION_MINECRAFT_FORGE_SERVER="1.21.1-52.0.9"
export VERSION_PODMAN_DESKTOP=1.12.0
export VERSION_GROOVY=4.0.22

# GraalVM Community
export GRAALVM_HOME=/opt/graalvm/graalvm-community-openjdk-$VERSION_GRAALVM_JAVA+$VERSION_GRAALVM_JAVA_PLUS

# Kafka home
export KAFKA_INSTALL_DIR="/opt/apache/kafka"
export KAFKA_HOME="$KAFKA_INSTALL_DIR/kafka_"$VERSION_KAFKA_SCALA"-$VERSION_KAFKA"

# JAVA
export JAVA_FOLDER="/opt/java"

export VERSION_JAVA_8="jdk8u422-b05"
export VERSION_JAVA_11="jdk-11.0.25+9"
export VERSION_JAVA_17="jdk-17.0.9+9"
export VERSION_JAVA_21="jdk-21.0.5+11"
export VERSION_JAVA_23="jdk-23+37"
export VERSION_JAVA_NEXT="jdk-next"

export JAVA_NEXT_HOME="$HOME/git/openjdk/jdk/build/linux-x86_64-server-release/jdk"
